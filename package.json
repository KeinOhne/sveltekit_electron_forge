{
	"name": "sveltekit-electron-forge",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"to_electron": "node to_electron.js",
		"electron_start": "npm --prefix ./electron run start",
		"electron_make": "npm --prefix ./electron run make",
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"devDependencies": {
		"@ptkdev/sveltekit-electron-adapter": "^0.3.0",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.3"
	},
	"type": "module"
}
